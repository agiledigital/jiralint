    "inquirer": "8.1.2",
    "io-ts": "2.2.16",
    "io-ts-types": "0.5.16",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.4",
    "oauth": "0.9.15",
    "readonly-types": "2.0.0",
    "string-length": "4.0.2",
    "ts-pattern": "3.2.4",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@types/clear": "0.1.2",
    "@types/clui": "0.3.1",
    "@types/inquirer": "7.3.3",
    "@types/jest": "26.0.24",
    "@types/oauth": "0.9.1",
