    "jest": "27.5.1",
    "prettier": "2.6.2",
    "rollup": "2.75.6",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "27.0.4",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "type-coverage": "2.18.0",
