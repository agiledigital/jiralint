{
  "name": "cli",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "NODE_ENV=production rollup -c --environment BUILD:production",
    "start": "NODE_ENV=development rollup -c",
    "watch": "npm run start -- -w",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives",
    "coverage": "type-coverage",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "echo 'add some tests'"
  },
  "dependencies": {
    "clear": "0.1.0",
    "clui": "0.3.6",
    "colors": "1.4.0",
    "date-fns": "2.29.2",
    "fp-ts": "2.12.3",
    "inquirer": "8.2.4",
    "io-ts": "2.2.18",
    "io-ts-types": "0.5.16",
    "monocle-ts": "2.3.13",
    "newtype-ts": "0.3.5",
    "oauth": "0.10.0",
    "readonly-types": "2.0.0",
    "string-length": "4.0.2",
    "ts-pattern": "3.3.5",
    "yargs": "17.5.1"
  },
  "devDependencies": {
    "@types/clear": "0.1.2",
    "@types/clui": "0.3.1",
    "@types/inquirer": "7.3.3",
    "@types/jest": "29.0.0",
    "@types/oauth": "0.9.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-typed-fp": "3.3.0",
    "eslint-plugin-functional": "4.2.2",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.15.0",
    "eslint-plugin-total-functions": "6.0.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "rollup": "2.79.0",
    "rollup-plugin-cleanup": "3.2.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-typescript2": "0.33.0",
    "ts-jest": "28.0.8",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "type-coverage": "2.22.0",
    "typescript": "4.8.2"
  },
  "typeCoverage": {
    "atLeast": 100,
    "ignoreCatch": false,
    "strict": true,
    "detail": true
  }
